Menu menu = new Menu();
SqlConnection conn = new SqlConnection(@"Data Source=AIRONE\SQLEXPRESS;Initial Catalog=LOGIN_FORM;Integrated Security=True;");
SqlDataAdapter sda = new SqlDataAdapter("SELECT COUNT(*) FROM Admin WHERE username = '"+userText.Text+"' AND password = '"+passText.Text+"'", conn);
DataTable dt = new DataTable();
sda.Fill(dt);
if (dt.Rows[0][0].ToString() == "1")
{
    this.Hide();
    menu.Show();
}
else
{
    MessageBox.Show("Incorrect password or username.", "ALERT", MessageBoxButtons.OK);
}
